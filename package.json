{
  "name": "gnl-react",
  "version": "1.0.0",
  "description": "detroitledger.org frontend",
  "main": "src",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "HTTPS=true yarn start",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "test": "react-scripts test --env=jsdom --coverage"
  },
  "engines": {
    "node": ">=10.0 <14"
  },
  "author": "Benjamin Chodoroff",
  "license": "MIT",
  "dependencies": {
    "@bnchdrff/react-user-tour": "^3.0.1",
    "apollo-boost": "0.4.4",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.4.2",
    "apollo-link-http": "^1.5.5",
    "axios": "^0.15.3",
    "config": "3.0.0",
    "cross-fetch": "^1.1.1",
    "graphql": "14.5.5",
    "graphql-tag": "2.10.0",
    "graphql-tools": "^0.9.0",
    "history": "^4.7.2",
    "http-proxy-middleware": "^0.20.0",
    "immutability-helper": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.1.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "node-sass": "^4.12.0",
    "normalizr": "^3.2.3",
    "numeral": "^2.0.4",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-apollo": "2.3.2",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.6.3",
    "react-dropdown": "^1.3.0",
    "react-helmet": "5.2.0",
    "react-pdf-js": "^2.0.3",
    "react-redux": "^4.4.6",
    "react-router-dom": "4.3.1",
    "react-router-redux": "next",
    "react-scripts": "3.1.1",
    "react-svg-chart": "4.0.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-form": "^6.2.1",
    "redux-mock-store": "^1.2.1",
    "redux-thunk": "^2.2.0",
    "toml": "2.3.3",
    "webpack-cli": "3.1.2"
  },
  "devDependencies": {
    "css-loader": "2.0.0",
    "enzyme": "^2.7.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.4",
    "nock": "^11.3.4",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "16.6.3",
    "rimraf": "^2.5.4",
    "sass-loader": "7.1.0",
    "stripcomment-loader": "^0.1.0",
    "style-loader": "0.23.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "import/no-named-as-default": 0,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "plugins": [
      "react"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
